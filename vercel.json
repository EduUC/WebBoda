{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist/cielitos" } },
    { "api": "api/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "api": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
