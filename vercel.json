{
  "version": 2,
  "builds": [
    {
      "src": "API/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "angular.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/API/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
